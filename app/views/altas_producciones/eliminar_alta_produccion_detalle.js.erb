<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-alta-produccion-detalle' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-alta-produccion");
   
    setTimeout(

      function(){ $("#msg-eliminar-alta-produccion-detalle").remove(); 
     
      $("#link-to-alta-produccion-detalle-<%=@alta_produccion_detalle_elim.alta_produccion_id %>").trigger('click');
      $("#link-to-alta-produccion-detalle-<%=@alta_produccion_detalle_elim.alta_produccion_id%>").trigger('click');
      
       swal("Alta Producción","Producción eliminado exitosamente.", "success");

    }, 1000);
   
    
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-alta-produccion-detalle' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%=@msg %></div>").insertBefore("#table-alta-produccion");
  setTimeout(function(){ 
    
    $("#msg-eliminar-alta-produccion-detalle").remove();

    swal("Alta Producción","No se pudo eliminar la Producción. <%=@msg%>. Intente más tarde", "error")

     }, 2000);
    
<% end %>