<% if @guardado_ok %>
 
  setTimeout( function(){ 
  
    $("#<%= params[:window_id]%>").modal('hide');
    swal("Alta Producción-Quesos","Producción Queso agregado exitosamente!", "success");

    }, 1000);

  setTimeout(function(){ location.href="<%= url_for(altas_producciones_index_path) %>"; }, 3000);
 
<% else %>

  swal("Alta Producción-Quesos", "Error al guardar la Producción. Intente más tarde","error");

<% end %>