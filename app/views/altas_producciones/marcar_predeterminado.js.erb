<% if @actualizado_ok %>

   $("<div class='col-sm-12' id='msg-actualizar-alta-produccion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Inactivado exitosamente.</div>").insertBefore("#table-alta-produccion");
   $("#form_buscar_alta_produccion_id").val('<%= @alta_produccion.id %>');
   $("#form_buscar_alta_produccion_periodo").val('');
   $("#form_buscar_alta_produccion_ganado_nombre").val('');
   $("#form_buscar_alta_produccion_ganado_rp").val('');
   $("#form-buscar-altas-producciones").submit();

    setTimeout(

      function(){ $("#msg-actualizar-alta-produccion").remove(); 
  
      
       swal("Alta Producción","Producción eliminado exitosamente.", "success");

    }, 1000);
   
    
<% else %>

  $("<div class='col-sm-12' id='msg-actualizar-alta-produccion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%=@msg %></div>").insertBefore("#table-alta-produccion");
  setTimeout(function(){ 
    
    $("#msg-actualizar-alta-produccion").remove();

    swal("Alta Producción","No se pudo desactivar la roducción. Intente más tarde", "error")

     }, 1000);
    
<% end %>