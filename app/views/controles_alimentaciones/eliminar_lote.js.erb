<% if @lote_eliminado %>

  $("<div class='col-sm-12' id='msg-eliminar-control-alimentacion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-control-alimentacion");
  $("#form_buscar_control_alimentacion_id").val("");
  $("#form_buscar_control_alimentacion_fecha_control").val('');
  $("#form_buscar_control_alimentacion_codigo").val("<%=params[:codigo_lote]%>");
  $("#form_buscar_control_alimentacion_nombre").val('');
  $("#form_buscar_control_alimentacion_ganado_rp").val('');
  $("#form-buscar-control-alimentacion").submit();
    setTimeout(function(){ 
      
      $("#<%= params[:window_id]%>").modal('hide');
      $("#msg-eliminar-control-alimentacion").remove();
       swal("Control Alimentación","Alimentación Eliminada Exitosamente!.", "success");

    }, 1000);
      
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-control-alimentacion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#table-control-alimentacion");
  setTimeout(function(){ 

    $("#msg-eliminar-control-alimentacion").remove(); 

    swal("Control Alimentación", "No se pudo eliminar la Alimentación. <%=@msg%>. Intente más tarde","error");
  
  }, 1000);
  
    
<% end %>