<% if @guardado_ok %>

  $("<div class='col-sm-12' id='msg-agregar-control-alimentacion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-control-alimentacion");
  
  <% if params[:clasificacion_alimentacion][:id].to_i == PARAMETRO[:clasificacion_por_ganado] %>
    
    $("#form_buscar_control_alimentacion_id").val('<%= @alimentacion.id %>');
  
  <%else%>

    $("#form_buscar_control_alimentacion_codigo").val('<%= params[:codigo_lote]%>');

  <%end%>
  
  $("#form_buscar_control_alimentacion_fecha_control").val('');
  $("#form_buscar_control_alimentacion_nombre").val('');
  $("#form_buscar_control_alimentacion_ganado_rp").val('');
  $("#form-buscar-control-alimentacion").submit();
  
  setTimeout(function(){ 
    $("#<%= params[:window_id]%>").modal('hide'); 
    swal("Sanitaciones","Sanidad agregada exitosamente!", "success");
  }, 2000);


<% else %>
  
    $("<div class='col-sm-12' id='msg-agregar-control-alimentacion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#form-agregar-control-alimentacion");
    swal("Sanitaciones", "Error al guardar la sanidad.<%= @msg %> Intente m√°s tarde","error");

<% end %>