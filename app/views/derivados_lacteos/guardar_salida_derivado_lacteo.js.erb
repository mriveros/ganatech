<% if @guardado_ok %>
 
  setTimeout( function(){ 
  
    $("#<%= params[:window_id]%>").modal('hide');
    $("#link-to-alta-produccion-detalle-<%= params[:alta_produccion_id]%>").trigger('click');
    $("#link-to-alta-produccion-detalle-<%= params[:alta_produccion_id]%>").trigger('click');

    swal("Alta Producción","Producción agregado exitosamente!", "success");

    }, 1000);

 
<% else %>

  swal("Alta Producción", "Error al guardar la Producción: <%=@msg%>. Intente más tarde","error");

<% end %>