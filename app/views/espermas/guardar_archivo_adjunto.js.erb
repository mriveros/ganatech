<% if @valido == true %>

  setTimeout(function(){

    $("#form_buscar_esperma_id").val('<%= @esperma.id %>');
  	$("#form_buscar_esperma_descripcion").val('');
  	$("#form_buscar_esperma_numero").val('');
  	$("#form_buscar_esperma_raza_id").val('');
    $("#form_buscar_esperma_cantidad").val('');
  	$("#form-buscar-esperma").submit();
    swal("Archivo Adjunto Exitosamente.", "", "success");

  }, 1000);
  

<% else %>
  
  swal({title: "Atención", text: " No se pudo adjuntar el archivo. Intente más tarde. <%= @msg %>", html: false});

<% end %>