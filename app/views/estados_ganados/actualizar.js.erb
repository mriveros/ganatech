<% if @estado_ganado_ok.present? %>

   $("<div class='col-sm-12' id='msg-editar-estado-ganado' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Actualizado Exitosamente.</div>").insertBefore("#form-editar-estado-ganado");

  $("#form_buscar_estados_ganados_id").val('<%= @estado_ganado.id %>');
  $("#form_buscar_estados_ganados_descripcion").val('');

  setTimeout(function(){

    $("#<%= params[:window_id]%>").modal('hide');
    $("#form-buscar-estados-ganados").submit();

    swal("El estado ganado ha sido actualizada Exitosamente.");

  }, 2000);

<% else %>

  $("<div class='col-sm-12' id='msg-editar-estado-ganado' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-editar-estado-ganado");

<% end %>
