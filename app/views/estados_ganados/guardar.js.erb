<% if @estado_ganado_ok %>

   $("<div class='col-sm-12' id='msg-agregar-estado-ganado' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-estado-ganado");

  $("#form_buscar_estados_ganados_id").val('<%= @estado_ganado.id %>');
  $("#form_buscar_estados_ganados_descripcion").val('');
  $("#form-buscar-estados-ganados").submit();

  setTimeout(function(){
    $("#<%= params[:window_id]%>").modal('hide');
    swal("El estado ganado ha sido guardado Exitosamente.");
  }, 2000);


<% else %>

	swal("Error al guardar:" + "<%=j(@msg)%>")
  $("<div class='col-sm-12' id='msg-agregar-estado-ganado' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-agregar-estado-ganado");

<% end %>
