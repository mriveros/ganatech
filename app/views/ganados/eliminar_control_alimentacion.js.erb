<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-control-alimentacion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-ganado-detalle-control-alimentacion");
   
    setTimeout(

      function(){ $("#msg-eliminar-control-alimentacion").remove(); 
     
      $("#link-to-ganado-detalle-<%=@control_alimentacion_elim.ganado_id %>").trigger('click');
      $("#link-to-ganado-detalle-<%=@control_alimentacion_elim.ganado_id%>").trigger('click');
      
       swal("Control Alimentación","Control Alimentación eliminado exitosamente.", "success");

    }, 1000);
   
    
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-control-alimentacion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%=@msg %></div>").insertBefore("#table-ganado-detalle-control-alimentacion");
  setTimeout(function(){ 
    
    $("#msg-eliminar-control-alimentacion").remove();

    swal("Control Alimentación","No se pudo eliminar el Control de Alimentación. Intente más tarde", "error")

     }, 2000);
    
<% end %>