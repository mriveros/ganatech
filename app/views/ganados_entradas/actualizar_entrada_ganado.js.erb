<% if @guardado_ok %>

   $("<div class='col-sm-12' id='msg-editar-ganado-entrada' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-editar-ganado-entrada");
   
  $("#form_buscar_ganado_entrada_id").val('<%= @ganado_entrada.id %>');
  $("#form_buscar_ganado_entrada_codigo_lote").val('');
   
  
  setTimeout(function(){ 

    $("#<%= params[:window_id]%>").modal('hide');
    $("#form-buscar-ganado-entrada").submit(); 
    swal("Entrada de Ganados modificado Exitosamente.")
    
}, 1000);


<% else %>
  
  $("<div class='col-sm-12' id='msg-editar-ganado-entrada' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#form-editar-ganado-entrada");
  
  setTimeout(function(){ 
    
    swal("Error al modificar la Entrada de Ganados. Intente m√°s tarde");
    
	}, 1000);


<% end %>