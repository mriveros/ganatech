<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-ganado' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-ganado");
   $("#form_buscar_ganado_id").val("<%=@ganado_elim.id%>")
   $("#form-buscar-ganado").submit();
    setTimeout(

      function(){ $("#msg-eliminar-ganado").remove(); 

       swal("El ganado fue eliminado Exitosamente.");

    }, 1000);
   
    
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-ganado' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#table-ganado");
  setTimeout(function(){ 
    
    $("#msg-eliminar-ganado").remove();

    swal("No se pudo eliminar el Ganado. Intente m√°s tarde")

     }, 2000);
  	
<% end %>
