<% if @actualizado %>
   
  $("#form_buscar_salida_ganado_id").val('<%= @ganado_salida.id %>');
  $("#form_buscar_salida_ganado_codigo_lote").val('');
  
  setTimeout(function(){ 
    
    $("#<%= params[:window_id]%>").modal('hide');
    $("#form-buscar-salida-ganado").submit();
    swal("Salida de Ganados Finalizado Exitosamente.")
    
}, 1000);


<% else %>
  
  setTimeout(function(){ 
    
    swal("Error al Finalizar la Salida de Ganados. Intente m√°s tarde. <%=@msg%>");
    
  }, 1000);


<% end %>