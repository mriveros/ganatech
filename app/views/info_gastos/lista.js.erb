$("#info-gastos-lista").html("<%= escape_javascript(render partial: "lista", locals: { info_gastos: @info_gastos }) %>");
function exportar_pdf_gastos()
  {
   	if(<%=@info_gastos.size%> == 0)
    {
    	swal("No hay datos para exportar")
    }
	else{

		window.open("<%= url_for(info_gastos_exportar_pdf_path(@parametros))%>", '_blank')	

	}
    
  }