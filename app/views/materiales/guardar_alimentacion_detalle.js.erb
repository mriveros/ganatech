<% if @guardado_ok %>

	$("<div class='col-sm-12' id='msg-agregar-alimentacion-detalle' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-alimentacion-detalle");

	setTimeout( function(){

    $("#<%= params[:window_id]%>").modal('hide');
    $("#form_buscar_alimentaciones_id").val('<%= @alimentacion.id %>');
    $("#form-buscar-alimentaciones").submit();
    
    swal("Suministro agregado exitosamente!");

    }, 1000);

<% else %>
	
	$("<div class='col-sm-12' id='msg-agregar-alimentacion-detalle' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#form-agregar-alimentacion-detalle");
  	swal('Error al guardar el suministro. Intente m√°s tarde');

<% end %>
