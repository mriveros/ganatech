<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-medicamento' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-medicamento");
   $("#form_buscar_medicamento_id").val("<%=@medicamento_elim.id%>")
   $("#form-buscar-medicamento").submit();
    setTimeout(function(){ 
      
      $("#msg-eliminar-medicamento").remove();
       swal("Medicamentos","Medicamento eliminado exitosamente!", "success");

    }, 1000);
      
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-medicamento' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#table-medicamento");
  setTimeout(function(){ 

    $("#msg-eliminar-medicamento").remove(); 

    swal("Medicamentos", "<%= @msg%>","error");
  
  }, 2000);
  
    
<% end %>
