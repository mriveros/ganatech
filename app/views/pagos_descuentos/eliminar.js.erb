<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-pago-descuento' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-pagos-descuentos");
   $("#form_buscar_pagos_descuentos_id").val("<%=@pago_descuento_elim.id%>")
   $("#form-buscar-pagos-descuentos").submit();
    
    setTimeout(function(){

      $("#msg-eliminar-pago-descuento").remove();
      
      swal("Descuentos","Registro de Descuento eliminado exitosamente.", "success");

    }, 1000);

<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-pago-descuento' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'>Error al Eliminar. Intente más Tarde</div>").insertBefore("#table-pagos-descuentos");
  
  setTimeout(function(){ 

  	$("#msg-eliminar-pago-descuento").remove(); 
	 swal("Descuentos", "No se pudo eliminar el Registro de Descuento. Intente más tarde.", "error");

	}, 1500);


    

<% end %>
