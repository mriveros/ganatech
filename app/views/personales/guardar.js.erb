<% if @guardado_ok %>

  $("<div class='col-sm-12' id='msg-agregar-personales' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#table-personal");
   
  $("#form_buscar_personales_id").val('<%= @personal.id %>');
  $("#form_buscar_personales_nombre").val('');
  $("#form_buscar_personales_apellido").val('');
  $("#form_buscar_personales_ruc_ci").val('');
  $("#form_buscar_personales_direccion").val('');
  $("#form_buscar_personales_telefono").val('');
  $("#form_buscar_personales_email").val('');
  $("#form_buscar_personales_cargo_id").val('');
  $("#form_buscar_personales_sueldo").val('');
  $("#form_buscar_personales_hacienda_id").val('');
  $("#form_buscar_personales_estado_personal_id").val('');
  
  $("#form-buscar-personales").submit();

  
  setTimeout(function(){ 
    
    $("#<%= params[:window_id]%>").modal('hide');
    swal("Personales","Personal agregado exitosamente!", "success");

    }, 1500);


<% else %>
	
  swal("Error al guardar:" + "<%=j(@msg)%>")
  $("<div class='col-sm-12' id='msg-agregar-personales' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#table-personal");

<% end %>
