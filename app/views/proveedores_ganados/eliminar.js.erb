<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-proveedor-ganado' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-proveedores-ganados");
   $("#form_buscar_proveedores-ganados_id").val("<%=@proveedor_ganado_elim.id%>")
   $("#form-buscar-proveedores-ganados").submit();
    setTimeout(function(){

      $("#msg-eliminar-proveedores-ganados").remove();
      swal("El proveedor de ganado ha sido eliminado Exitosamente.");

    }, 1000);

<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-proveedor-ganado' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg).html_safe %></div>").insertBefore("#table-proveedores-ganados");
  setTimeout(function(){ $("#msg-eliminar-proveedor-ganado").remove(); }, 1000);

    swal("no se pudo Eliminar el proveedor de ganado. Intente m√°s tarde.")

<% end %>
