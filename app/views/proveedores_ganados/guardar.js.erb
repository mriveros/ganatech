<% if @proveedor_ganado_ok %>

$("<div class='col-sm-12' id='msg-agregar-proveedor-ganado' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-proveedor-ganado");

$("#form_buscar_proveedores_ganados_id").val('<%= @proveedor_ganado.id %>');
$("#form_buscar_proveedores_ganados_nombre_razon_social").val('');
$("#form_buscar_proveedores_ganados_ruc_ci").val('');
$("#form_buscar_proveedores_ganados_direccion").val('');
$("#form_buscar_proveedores_ganados_telefono").val('');
$("#form_buscar_proveedores_ganados_email").val('');
$("#form_buscar_proveedores_ganados_observacion").val('');


$("#form-buscar-proveedores_ganados").submit();

setTimeout(function(){
    $("#<%= params[:window_id]%>").modal('hide');
    swal("El provedor de ganado ha sido guardada Exitosamente.");
}, 2000);


<% else %>

swal("Error al guardar:" + "<%=j(@msg)%>")
$("<div class='col-sm-12' id='msg-agregar-proveedor-ganado' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-agregar-proveedor-ganado");

<% end %>
