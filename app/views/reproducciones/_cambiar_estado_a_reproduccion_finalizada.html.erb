<div class="modal-dialog">
      
  <div class="modal-content">
        
    <div class="modal-header" style="background:#e6e6e6;">
          
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
      <h4 class="modal-title" id="myModalLabel">Finalizar Reproducción</h4>
      
    </div>


    <%= form_for @reproduccion, remote: true, url: reproducciones_guardar_cambiar_estado_a_reproduccion_finalizada_url, html: { id: "form-cambiar-estado-reproduccion" }, method: 'POST' do |f| %>

      <%= hidden_field_tag :window_id, params[:window_id]%>
      <%= hidden_field_tag :celo_id, @reproduccion.celo_id%>
      <%= hidden_field_tag :reproduccion_id, @reproduccion.id%>
      

      <div class="modal-body">

        <div class="panel-body">

          <div class="col-sm-4"><%= label_tag nil, "Tipo Concepción #{attr_requerido}".html_safe %></div>
          <div class="col-sm-7"><%= f.collection_select :tipo_concepcion_id, TipoConcepcion.tipo_concepcion.orden_01, :id, :descripcion, { include_blank: true}, { class: 'form-control input-sm', style: "width:200px;", disabled: true }%></div>
          <div class="col-sm-1"></div><br/><br/>

            <div class="col-sm-4"><%= f.label :nombre_lbl, "Nombre Ganado ".html_safe %></div>
            <div class="col-sm-7"><%= f.text_field :celo_ganado_nombre,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />  
            

            <div class="col-sm-4"><%= f.label :rp_lbl, "RP ".html_safe %></div>
            <div class="col-sm-7"><%= f.text_field :celo_ganado_rp,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />


            <div class="col-sm-4"><%= f.label :rfid_lbl, "Código RFID ".html_safe %></div>
            <div class="col-sm-7"><%= f.text_field :celo_ganado_rfid, class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />
          
          <div id = "monta_natural" hidden="true">
            
            <div class="col-sm-4"><%= f.label :nombre_lbl, "Nombre Reproductor ".html_safe %></div>
            <div class="col-sm-7"><%=text_field_tag :nombre_ganado, nil,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />  
            

            <div class="col-sm-4"><%= f.label :rp_lbl, "RP ".html_safe %></div>
            <div class="col-sm-7"><%= text_field_tag :rp, nil,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />


            <div class="col-sm-4"><%= f.label :rfid_lbl, "Código RFID ".html_safe %></div>
            <div class="col-sm-7"><%= text_field_tag :codigo_rfid, nil,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />
          
          </div>


          <div id = "inseminacion_artificial" hidden="true">

            <%= hidden_field_tag :esperma_id %>
            <div class="col-sm-4"><%= label_tag :nil, "Muestra(Esperma)#{attr_requerido}".html_safe %></div>
            <div class="col-sm-7"><%= text_area_tag :esperma ,nil,class: 'form-control input-sm' %></div>
            <div class="col-sm-1"></div><br /><br />

            <div class="col-sm-4"><%= f.label :procedencia_lbl, "Procedencia ".html_safe %></div>
            <div class="col-sm-7"><%= text_field_tag :procedencia_muestra, nil,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />  

            <div class="col-sm-4"><%= f.label :nombre_lbl, "Nombre Ganado ".html_safe %></div>
            <div class="col-sm-7"><%= text_field_tag :nombre_ganado_muestra, nil,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />  
            
            <div class="col-sm-4"><%= f.label :raza_lbl, "Raza ".html_safe %></div>
            <div class="col-sm-7"><%= text_field_tag :raza_muestra, nil,class: 'form-control input-sm',style:"text-transform:uppercase;", readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />

            <div class="col-sm-4"><%= f.label :numero_muestra_lbl, "Número Muestra".html_safe %></div>
            <div class="col-sm-7"><%= text_field_tag :numero_muestra, nil,class: 'form-control input-sm', readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />

            <div class="col-sm-4"><%= f.label :cantidad_muestra_lbl, "Cantidad".html_safe %></div>
            <div class="col-sm-7"><%= text_field_tag :cantidad_muestra, nil,class: 'form-control input-sm', readonly: true %></div>
            <div class="col-sm-1"></div><br /><br />


          </div>

          <div class="col-sm-4"><%= label_tag :nil, "Fecha Concepción #{attr_requerido}".html_safe %></div>
          <div class="col-sm-7"><%= date_field_tag :fecha_reproduccion, Date.today.strftime('%d/%m/%Y'), class: 'form-control input-xs' %>
          </div>    
          <div class="col-sm-1"></div><br /><br />  


          <div class="col-sm-4"><%= label_tag :nil, "Descripción #{attr_requerido}".html_safe %></div>
          <div class="col-sm-7"><%= text_field_tag :descripcion ,nil,class: 'form-control input-sm' %></div>
          <div class="col-sm-1"></div><br /><br />


          <div class="col-sm-4"><%= label_tag :nil, "Observación ".html_safe %></div>
          <div class="col-sm-7"><%= text_area_tag :observacion ,nil,class: 'form-control input-sm' %></div>
          <div class="col-sm-1"></div><br />
                 
           
       
      </div>

      <div class="modal-footer" style="background:#e6e6e6;">

        <%= link_to "Cerrar", "#", class: "btn btn-default", onclick: "$('##{params[:window_id]}').modal('hide');" %>

        <%= link_to "Finalizar","#", class: "btn btn-primary", id: "btn-guardar-cambiar-estado" %>
          
      </div>
    
    <% end %>

  </div>
</div>

<style type="text/css">

  #form-cambiar-estado-reproduccion label, #form-cambiar-estado-reproduccion input[type='text'] { float:left; margin-top:5px;}
  #form-cambiar-estado-reproduccion br {clear:both; }
  #form-cambiar-estado-reproduccion label { width:200px; font-size:12px;}
  .ui-autocomplete{z-index: 1000000; font-size: 10px;}

</style>

<script type="text/javascript">



  aplicar_datepicker2();

  setTimeout(function(){ 

    $("#fecha_inicio").focus();
   
  }, 1000);

  if ($("#v_reproduccion_tipo_concepcion_id").val() == <%=PARAMETRO[:tipo_concepcion_monta_natural]%>){

    $("#monta_natural").show();
    $("#inseminacion_artificial").hide();

  } else{
    alert("sd")
    $("#inseminacion_artificial").show();
    $("#monta_natural").hide();

  } 


   $("textarea").keydown(function(e){
    // Enter was pressed without shift key
    if (e.keyCode == 13 && !e.shiftKey)
    {
        // prevent default behavior
        e.preventDefault();
    }
    });

   function isNumberKey(evt)
  {
     var charCode = (evt.which) ? evt.which : event.keyCode
     if (charCode > 31 && (charCode < 48 || charCode > 57) && charCode != 46) {
         
         return false;
     }

     return true;
  }

  $("#btn-guardar-cambiar-estado").bind('click', function () {

      valido = true
      msg = ""

      if($("#tipo_concepcion_id").val().length == 0){

        msg +="Debe seleccionar un tipo de Concepción.\n"
        valido = false
          
      }

     if($("#tipo_concepcion_id").val() == <%=PARAMETRO[:tipo_concepcion_monta_natural]%>){

      if($("#ganado_reproductor_id").val().length == 0){

        msg +="Debe seleccionar un Ganado como Reproductor.\n"
        valido = false
          
      }

     }
      else{

        if($("#esperma_id").val().length == 0){

          msg +="Debe seleccionar una Muestra para la Reproducción.\n"
          valido = false
          
        }
          
      }
      
      if($("#fecha_reproduccion").val().length == 0){

        msg +="Debe agregar una fecha de Reproducción.\n"
        valido = false
          
      }

      if($("#descripcion").val().length == 0){

        msg +="Debe agregar una Descripción.\n"
        valido = false
          
      }
    

      if (valido){

        swal({
          title: "Confirmar Reproducción",
          text: "Al confirmar el estado del Celo a Reproducción éste podrá ser visualizado en el módulo de Reproducciones para su seguimiento durante el proceso de preñez.",
          type: "warning",
          showCancelButton: true,
          confirmButtonClass: "btn-confirm",
          confirmButtonText: "Confirmar",
          cancelButtonText: "Cancelar",
          closeOnConfirm: true,
          closeOnCancel: true
        },
        function(isConfirm) {
          if (isConfirm) {

            $("#form-cambiar-estado-reproduccion").submit();
            
          } 

        });

      }else{

        swal(msg);
      
      }

      return false;
    
    })




</script>