<% if @guardado_ok %>
   
  $("#form_buscar_reproduccion_id").val('<%= @reproduccion.id %>');
  $("#form_buscar_reproduccion_ganado_rfid").val('');
  $("#form_buscar_reproduccion_ganado_nombre").val('');
  $("#form_buscar_reproduccion_ganado_rp").val('');
 
  
  setTimeout(function(){ 

    
    swal("Reproducción","Estado de Reproducción modificado exitosamente!", "success");
    $("#form-buscar-reproduccion").submit();
    
  }, 2000);


<% else %>
  
  swal("Reproducción", "Error al guardar el cambio de estado de Reproducción.<%=@msg%> Intente más tarde","error");

<% end %>