<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-reproduccion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-reproduccion");
   $("#form_buscar_reproduccion_id").val("<%=@reproduccion_elim.id%>")
   $("#form-buscar-reproduccion").submit();
    setTimeout(function(){ 
      
      $("#msg-eliminar-reproduccion").remove();
       swal("Reproducción","Reproducción eliminado exitosamente! El estado del ganado cambiado a En Celo(Celo Activo).", "success");

    }, 1000);
      
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-reproduccion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#table-reproduccion");
  setTimeout(function(){ 

    $("#msg-eliminar-reproduccion").remove(); 

    swal("Reproducción", "No se pudo eliminar la Reproducción. Intente más tarde","error");
  
  }, 2000);
  
    
<% end %>
