<% if @guardado_ok %>

   $("<div class='col-sm-12' id='msg-agregar-celo' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-celo");
   
  $("#form_buscar_celo_id").val('<%= @celo.id %>');
  $("#form_buscar_celo_ganado_rfid").val('');
  $("#form_buscar_celo_ganado_nombre").val('');
  $("#form_buscar_celo_ganado_rp").val('');
  $("#form-buscar-celo").submit();
  
  setTimeout(function(){ 
    $("#<%= params[:window_id]%>").modal('hide'); 
    swal("Celos","Celo agregado exitosamente!", "success");
  }, 2000);


<% else %>
  
    $("<div class='col-sm-12' id='msg-agregar-celo' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#form-agregar-celo");
    swal("Celos", "Error al guardar el Celo.<%=@msg%> Intente m√°s tarde","error");

<% end %>