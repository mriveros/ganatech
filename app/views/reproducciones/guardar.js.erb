<% if @guardado_ok %>
  

  $("<div class='col-sm-12' id='msg-agregar-reproduccion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-reproduccion");
   
  $("#form_buscar_reproduccion_id").val('<%= @reproduccion.id %>');
  $("#form_buscar_reproduccion_ganado_rfid").val('');
  $("#form_buscar_reproduccion_ganado_nombre").val('');
  $("#form_buscar_reproduccion_ganado_rp").val('');
 
  
  setTimeout(function(){ 

    $("#<%= params[:window_id]%>").modal('hide'); 
    swal("Reproducción","Reproducción Finalizada exitosamente!", "success");
    $("#form-buscar-reproduccion").submit();
    
  }, 2000);


<% else %>

    $("<div class='col-sm-12' id='msg-agregar-reproduccion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= @msg %></div>").insertBefore("#form-agregar-reproduccion");
    swal("Reproducción", "Error al finalizar la Reproducción.<%=@msg%> Intente más tarde","error");

<% end %>