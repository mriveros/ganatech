<% if @guardado_ok %>
   
  $("#form_buscar_reproduccion_id").val('<%= @reproduccion.id %>');
  $("#form_buscar_reproduccion_ganado_rfid").val('');
  $("#form_buscar_reproduccion_ganado_nombre").val('');
  $("#form_buscar_reproduccion_ganado_rp").val('');
 
  $("<div class='col-sm-12' id='msg-finalizar-reproduccion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-finalizar-reproduccion");

  setTimeout(function(){ 

    $("#<%= params[:window_id]%>").modal('hide'); 
    swal("Reproducción","Reproducción Finalizada exitosamente!", "success");
    $("#form-buscar-reproduccion").submit();
    
  }, 1500);


<% else %>
    
    $("<div class='col-sm-12' id='msg-finalizar-reproduccion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'>No se pudo Finalizar la Reproducción</div>").insertBefore("#form-finalizar-reproduccion");
    swal("Reproducción", "Error al finalizar la Reproducción.<%=@msg%> Intente más tarde","error");

<% end %>