  $("#<%= params[:window_id]%>").modal('hide');
  $("#form_buscar_derivados_lacteos_id").val('<%=@derivado_lacteo.id%>');

  setTimeout( function(){ 

  	
    swal("Derivados Lacteos","Salida Exitosa!", "success");

    }, 1000);

 	$("#form-buscar-derivados-lacteos").submit();

<% else %>
